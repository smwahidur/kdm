@model IEnumerable<KDM.tbl_binary_matching_data>

@{
    ViewBag.Title = "CustomerBinaryReportAction";
    Layout = "~/Views/Shared/_LayoutFramePage.cshtml";
}

<div class="card-body border-success">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PlacementID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BPCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProcessDate)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.BLeftPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BRightPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MatchingCount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ALeftPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ARightPoint)
            </th>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PlacementID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BPCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProcessDate)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.BLeftPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BRightPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MatchingCount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ALeftPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ARightPoint)
                </td>

            </tr>
        }

    </table>
</div>

